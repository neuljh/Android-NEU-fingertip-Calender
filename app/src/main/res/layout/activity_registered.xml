<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@mipmap/regis_bg"
    tools:context=".Activity.RegisteredActivity">

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="match_parent"
        android:layout_height="155dp"
        app:srcCompat="@mipmap/neu" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="244dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/corners_bg_touming"
            android:orientation="vertical">

            <EditText
                android:id="@+id/et_username_r"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/corners_bg"
                android:drawableStart="@drawable/ic_person"
                android:drawableEnd="@drawable/ic_star"
                android:drawableTint="@color/white_bg"
                android:textColorHint="@color/white_bg"
                android:textColor="@color/white_bg"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="请输入用户名"
                android:inputType="textPersonName"
                android:padding="16dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <EditText
                android:id="@+id/et_email_r"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/corners_bg"
                android:drawableTint="@color/white_bg"
                android:textColorHint="@color/white_bg"
                android:textColor="@color/white_bg"
                android:drawableStart="@drawable/ic_email"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="请输入邮箱"
                android:inputType="textPersonName"
                android:padding="16dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <EditText
                android:id="@+id/et_phonenum_r"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/corners_bg"
                android:drawableStart="@drawable/ic_phone"
                android:drawableEnd="@drawable/ic_star"
                android:drawableTint="@color/white_bg"
                android:textColorHint="@color/white_bg"
                android:textColor="@color/white_bg"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="请输入手机号"
                android:inputType="textPhonetic"
                android:padding="16dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <EditText
                android:id="@+id/et_password_r"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/corners_bg"
                android:drawableTint="@color/white_bg"
                android:textColorHint="@color/white_bg"
                android:textColor="@color/white_bg"
                android:drawableStart="@drawable/ic_lock_login"
                android:drawableEnd="@drawable/ic_star"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="请输入密码"
                android:inputType="textPassword"
                android:padding="16dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <EditText
                android:id="@+id/et_password_rr"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/corners_bg"
                android:drawableTint="@color/white_bg"
                android:textColorHint="@color/white_bg"
                android:textColor="@color/white_bg"
                android:drawableStart="@drawable/ic_lock_login"
                android:drawableEnd="@drawable/ic_star"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="请再次输入密码"
                android:inputType="textPassword"
                android:padding="16dp" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <EditText
                android:id="@+id/et_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/corners_bg"
                android:drawableTint="@color/white_bg"
                android:textColorHint="@color/white_bg"
                android:textColor="@color/white_bg"
                android:drawableStart="@drawable/ic_name"
                android:drawableEnd="@drawable/ic_star"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="请输入姓名"
                android:inputType="textPersonName"
                android:padding="16dp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <EditText
                android:id="@+id/et_school"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/corners_bg"
                android:drawableTint="@color/white_bg"
                android:textColorHint="@color/white_bg"
                android:textColor="@color/white_bg"
                android:drawableStart="@drawable/ic_school"
                android:drawableEnd="@drawable/ic_star"
                android:drawablePadding="16dp"
                android:ems="10"
                android:hint="请输入学校全称"
                android:inputType="textPersonName"
                android:padding="16dp" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <RadioGroup
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:background="@drawable/corners_bg"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="14dp"
                    android:layout_height="29dp"
                    android:layout_weight="0" />

                <TextView
                    android:id="@+id/text_sex"
                    android:layout_width="110dp"
                    android:layout_height="62dp"
                    android:layout_weight="1"
                    android:drawableStart="@drawable/ic_sex"
                    android:gravity="center"
                    android:text="性别:"

                    android:drawableTint="@color/white_bg"
                    android:textColorHint="@color/white_bg"
                    android:textColor="@color/white_bg"
                    android:textSize="18sp" />

                <RadioButton
                    android:id="@+id/rb_male"
                    android:layout_width="124dp"
                    android:layout_height="60dp"
                    android:gravity="center"
                    android:text="男"
                    android:textColor="@color/white_bg"/>

                <RadioButton
                    android:id="@+id/rb_female"
                    android:layout_width="126dp"
                    android:layout_height="68dp"
                    android:gravity="center"
                    android:text="女"
                    android:textColor="@color/white_bg" />
            </RadioGroup>

        </LinearLayout>
    </ScrollView>
    <TextView
        android:id="@+id/textView200"
        android:layout_width="match_parent"
        android:layout_height="20dp" />

    <EditText
        android:id="@+id/et_yanzhengma"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/corners_bg"
        android:drawableStart="@drawable/ic_person"
        android:drawableEnd="@drawable/ic_star"
        android:drawableTint="@color/white_bg"
        android:textColorHint="@color/white_bg"
        android:textColor="@color/white_bg"
        android:drawablePadding="16dp"
        android:ems="10"
        android:hint="请输入验证码"
        android:inputType="textPersonName"
        android:padding="16dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="20dp" />

    <Button
        android:id="@+id/bt_getnumber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/corners_bg"
        android:clickable="true"
        android:text="获取验证码"
        android:textColor="@color/white_bg"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView201"
        android:layout_width="match_parent"
        android:layout_height="6dp" />

    <Button
        android:id="@+id/bt_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/corners_bg"
        android:clickable="true"
        android:text="返回"
        android:textColor="@color/white_bg"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="6dp" />

    <Button
        android:id="@+id/bt_r_r"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/corners_bg"
        android:clickable="true"
        android:text="注册"
        android:textColor="@color/white_bg"
        android:textSize="18sp" />
</LinearLayout>